          <scanMaskLineSpacing>1</scanMaskLineSpacing>
          <scanMaskUserDefined>0</scanMaskUserDefined>
          <sensorIndex>-1</sensorIndex>
        </detector>
        <optics>
          <aspectRatio>1.33</aspectRatio>
