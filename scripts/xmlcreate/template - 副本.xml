        <enableACCoupling>0</enableACCoupling>
        <enableBlackHot>0</enableBlackHot>
        <enableAGC>0</enableAGC>
        <enableMotionBlur>0</enableMotionBlur>
        <sensorIndex>-1</sensorIndex>
        <detector>
          <startWavelength>3.5</startWavelength>
          <endWavelength>4.5</endWavelength>
          <dwellTime>100</dwellTime>
          <NET>0.5</NET>
          <FixedNoisePercent>0</FixedNoisePercent>
