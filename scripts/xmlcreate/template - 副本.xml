          <UserDefinedMTFenabIe>0</UserDefinedMTFenabIe>
          <UserDefinedMTFfileName/>
          <sensorIndex>-1</sensorIndex>
        </optics>
        <eletronics>
          <frameRate>30</frameRate>
          <preCutonFreq>0.001</preCutonFreq>
          <postCutoffFreq>14920</postCutoffFreq>
          <gain>1</gain>
          <level>0</level>
          <percentGain>1</percentGain>
          <percentLevel>0</percentLevel>
          <sensorIndex>-l</sensorIndex>
          <maxAGCgain>100</maxAGCgain>
          <minAGCgain>0.1</minAGCgain>
          <AGCmode>1</AGCmode>
          <colorMapType>1</colorMapType>
          <colorMapUserDefinedRadFile/>
          <colorMapEnable>0</colorMapEnable>
        </eletronics>
      </senSim>
    </sensor>
